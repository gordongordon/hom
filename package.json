{
  "name": "mobx-react-ant-boilerplate",
  "version": "1.0.1",
  "description": "MobX Boilerplate with Ant.Design",
  "scripts": {
    "start": "node server.js",
    "build": "webpack",
    "make": "npm run build && node server.js",
    "static": "node serverwebdev.js",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yonibot/mobx-react-ant-boilerplate"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Yoni Weisbrod <yoni@fastmail.com> (http://github.com/yonibot)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx/issues"
  },
  "engines": {
    "node": "6.10.0"
  },
  "homepage": "http://mobxjs.github.com/mobx",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.22.0",
    "babel-runtime": "6.23.0",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.9.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.1.1",
    "jest": "^19.0.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "node-sass": "^4.3.0",
    "postcss-loader": "^2.0.6",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "^0.3.1",
    "webpack": "^1.15.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^1.16.5"
  },
  "dependencies": {
    "antd": "^2.12.6",
    "antd-mobile": "^1.6.2",
    "compression": "^1.7.0",
    "firebase": "^4.1.3",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "mobx-react-devtools": "^4.2.15",
    "mobx-router": "0.0.7",
    "moment": "^2.18.1",
    "path-to-regexp": "^1.7.0",
    "rc-form": "^1.4.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-fastclick": "^3.0.2",
    "uuid": "^3.1.0",
    "validatorjs": "^3.13.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/tests/jestsetup.js"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/public/**"
    ],
    "coverageDirectory": "<rootDir>/src/test",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/public"
    ],
    "moduleFileExtensions": [
      "web.jsx",
      "web.js",
      "js",
      "jsx",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "build",
      "public",
      "other",
      "dist"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>src",
      "components",
      "data",
      "store",
      "firebase",
      "mobxs",
      "tests"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/",
      "node_modules/antd-mobile/lib/icon/index.web.js"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
